<Window x:Class="ZipCode_TextBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZipCode_TextBox"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="ZipCodeTextBox" HorizontalAlignment="Left" Height="57" Margin="88,75,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="310" TextChanged="ZipCodeTextBox_TextChanged"/>
        <Button x:Name="SubmitButton" IsEnabled="False" Content="Submit" HorizontalAlignment="Left" Margin="88,171,0,0" VerticalAlignment="Top" Width="149" Height="45"/>
        <Label x:Name="PScodelabel" Content="Please Enter A Postal Code" HorizontalAlignment="Left" Margin="88,46,0,0" VerticalAlignment="Top" Width="310"/>

    </Grid>
</Window>
